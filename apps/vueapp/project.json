{
  "name": "vueapp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/vueapp/src",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build": {
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/dist/apps/vueapp"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "../../dist/apps/vueapp"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "outputs": [
        "{projectRoot}/{options.coverage.reportsDirectory}",
        "{workspaceRoot}/coverage/apps/vueapp"
      ],
      "options": {
        "coverage.reportsDirectory": "../../coverage/apps/vueapp"
      }
    }
  }
}
