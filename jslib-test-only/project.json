{
  "name": "jslib-test-only",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "jslib-test-only/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/jslib-test-only",
        "main": "jslib-test-only/src/index.ts",
        "tsConfig": "jslib-test-only/tsconfig.lib.json",
        "assets": ["jslib-test-only/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "outputs": [
        "{projectRoot}/{options.coverage.reportsDirectory}",
        "{workspaceRoot}/coverage/jslib-test-only"
      ],
      "options": {
        "coverage.reportsDirectory": "../coverage/jslib-test-only"
      }
    }
  }
}
